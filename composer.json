{
  "name": "hafael/fitbank-php-sdk",
  "description": "Fitbank SDK for PHP",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "keywords": [
    "Fitbank",
    "PHP",
    "API",
    "client",
    "SDK",
    "BaaS",
    "CaaS",
    "banking",
    "open finance",
    "payments",
    "PIX",
    "boleto",
    "cards",
    "withdrawal",
    "TED",
    "P2P",
    "onboarding",
    "ispb"
  ],
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "3.*",
    "phpstan/phpstan": "1.11.x-dev"
  },
  "authors": [
    {
      "name": "Jordan Carvalho",
      "email": "suporte@paguesafe.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Paguesafe\\Fitbank\\": [
        "src",
        "test"
      ]
    }
  },
  "scripts": {
    "phpstan": "./vendor/bin/phpstan analyse -l 6 src"
  }
}
